<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="SBCSOATPolicyDataEndPoint" xmlns="http://ws.apache.org/ns/synapse">
    <address format="soap11" uri="$FILE:SBCSOATPolicyDataAddressUrl">
        <timeout>
            <duration>20000</duration>
            <responseAction>fault</responseAction>
        </timeout>
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </address>
</endpoint>
